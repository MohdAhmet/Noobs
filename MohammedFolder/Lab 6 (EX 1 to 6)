pragma solidity >=0.5.0 <0.9.0;
//Ex2
contract Factroy {
    uint idDigits = 16;
//Ex3
    uint idModulus = 10**idDigits;
//Ex4
struct Product {
uint id;
string name;
}
//Ex5
Product[] products;

//Ex6
function createProduct (uint _id, string memory _name) private {
    Product memory newProduct = Product(_id, _name);
    products.push(newProduct);
}

}
